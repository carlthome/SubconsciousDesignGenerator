<Window x:Class="SubconsciousDesignGenerator.CompositeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Composite"
        Loaded="onLoaded"
        Cursor="None"
        WindowStyle="None"
        ResizeMode="NoResize"
        VerticalContentAlignment="Stretch"
        HorizontalContentAlignment="Center"
        DataContextChanged="onDataContextChanged">
    <Viewbox Margin="20">
        <Grid Name="CompositeImage" Width="3307" Height="4677" Background="White">
            <Border BorderBrush="Black" BorderThickness="1">
                <Canvas ClipToBounds="True" Name="Layout"/>
            </Border>
            <Border VerticalAlignment="Bottom" Background="White" Padding="20" BorderBrush="Black" BorderThickness="1, 0, 1, 1">
                <StaticResource ResourceKey="DataTable" />
            </Border>
        </Grid>
    </Viewbox>
</Window>
