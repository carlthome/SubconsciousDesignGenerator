<Window x:Class="SubconsciousDesignGenerator.GazeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        ResizeMode="NoResize"
        WindowStyle="None"
        WindowState="Maximized"
        Topmost="True"
        Cursor="None"
        Closing="onClosing"
        ContentRendered="onContentRendered"
        Name="Gaze">
    <Grid>
        <TextBlock Text="Default" Style="{StaticResource LargeText}" Name="Instructions" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center" />
    </Grid>
</Window>
